USE db_login;

CREATE TABLE users (
    id INT AUTO_INCREMENT PRIMARY KEY,
    username VARCHAR(50) UNIQUE NOT NULL,
    password VARCHAR(255) NOT NULL,
    nama VARCHAR(100),
    email VARCHAR(100),
    telepon VARCHAR(20)
);

INSERT INTO users (username, password, nama, email, telepon) VALUES
